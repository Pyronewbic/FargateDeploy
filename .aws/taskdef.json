{
    "family": "sample-fargate",
    "executionRoleArn": "arn:aws:iam::046074525506:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "sap",
            "image": "sap",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ]
}